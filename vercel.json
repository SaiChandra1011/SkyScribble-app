{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "github": {
    "enabled": true,
    "silent": true,
    "autoJobCancelation": true
  },
  "git": {
    "deploymentEnabled": {
      "main": false,
      "new-main": false,
      "new-main-branch": false,
      "working-version": true
    }
  }
} 